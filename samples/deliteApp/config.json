{
	"id": "deliteApp",

	"loaderConfig": {
		"paths": {
			"has" : {
				"requirejs" : {
					"deliteApp": "../deliteApp"
				},
				"!requirejs" : {
					"deliteApp": "../dapp/samples/deliteApp"
				}
			}
		}
	},

	"containerSelector" : "#main",

	"modules": [
	],

	"parseOnLoad": true,

	"dependencies": [
		"deliteful/LinearLayout",
		"deliteful/Button",
		"deliteful/ViewStack"
	],

	"controllers": [
		"dapp/controllers/delite/Init",
		"dapp/controllers/delite/Load",
		"dapp/controllers/delite/Transition",
		"dapp/controllers/delite/Layout"
	],


	"defaultView": "header+content,home",

	"views": {
		"main": {
			"template": "deliteApp/main.html"
		},
		"header": {
			"constraint": "main",
			"parentSelector": "#main",
			"template": "deliteApp/header.html"
		},
		"home2": {
			"constraint": "main-view-stack",
			"parentSelector": "#main-view-stack",
			"controller" : "deliteApp/home.js",
			"template": "deliteApp/home.html",
			"nls": "deliteApp/nls/home"
		},
		"detail2": {
			"constraint": "main-view-stack",
			"parentSelector": "#main-view-stack",
			"controller" : "deliteApp/detail.js",
			"template": "deliteApp/detail.html"
		},
		"content": {
			"dependencies": [
				"deliteful/ViewStack"
			],
			"constraint": "main",
			"parentSelector": "#main",
			"template": "deliteApp/content.html",
			"defaultView": "home",
			"views": {
				"home": {
					"constraint": "main-view-stack",
					"parentSelector": "#main-view-stack",
					"controller" : "deliteApp/home.js",
					"template": "deliteApp/home.html",
					"nls": "deliteApp/nls/home"
				},
				"detail": {
					"constraint": "main-view-stack",
					"parentSelector": "#main-view-stack",
					"controller" : "deliteApp/detail.js",
					"template": "deliteApp/detail.html"
				}
			}
		},
		"p1": {
			"controller" : "deliteApp/home.js",
			"template": "deliteApp/home.html",
			"views": {
				"s1": {
					"constraint": "s1constraint",
					"parentSelector": "#s1constraint",
					"controller" : "deliteApp/home.js",
					"template": "deliteApp/home.html",
					"views": {
						"child1": {
							"constraint": "child1constraint",
							"parentSelector": "#child1constraint",
							"controller" : "deliteApp/home.js",
							"template": "deliteApp/home.html"
						}
					}
				}
			}
		}
	}
}
